		});
		
		dane.rok.addEvent('change',function() {
			dane.button.fireEvent('click');
		});		
		
		dane.button.addEvent('click',function() {
			othis.zakres(dane,0)
		});	

		if (dane.prev) {
			dane.prev.addEvent('click',function() {
				othis.zakres(dane,-1);
				return false;
			});	
		}
		if (dane.next) {
