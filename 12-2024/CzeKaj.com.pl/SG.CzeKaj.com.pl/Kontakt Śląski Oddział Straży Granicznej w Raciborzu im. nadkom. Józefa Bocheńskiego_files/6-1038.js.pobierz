				method: 'get', 
				url: link,
			    onRequest: function() {
			    	divdane.set('html','<div class="ajaxloading"><img src="/img/ajax/loading1.gif" style="vertical-align:middle;"> pobieranie danych ... </div>');
			    },
				onSuccess: function(responseTree, responseElements, responseHTML, responseJavaScript) {
					divdane.empty();
